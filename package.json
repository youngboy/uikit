    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.5.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.1.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-test-renderer": "^16.5.0",
